<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.admin.dao.LoginDao">
    <select id="checkLogin" parameterType="com.qf.admin.pojo.po.People" resultType="com.qf.admin.pojo.po.People">

     select  * from user where uname=#{uname} and pwd = #{pwd}
    </select>

</mapper>